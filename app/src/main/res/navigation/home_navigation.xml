<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/home_navigation"
    app:startDestination="@id/fragmentCbtActivities">


    <fragment
        android:id="@+id/fragmentCbtActivities"
        android:name="com.loopbreakr.brainstruct.UiHomeFragment"
        android:label="fragment_cbt_activities"
        tools:layout="@layout/fragment_ui_home" >
        <action
            android:id="@+id/action_fragmentCbtActivities_to_TJViewFragment"
            app:destination="@id/TJViewFragment" />
        <action
            android:id="@+id/action_fragmentCbtActivities_to_PCViewFragment"
            app:destination="@id/PCViewFragment" />
        <action
            android:id="@+id/action_fragmentCbtActivities_to_HIGHViewFragment"
            app:destination="@id/HIGHViewFragment" />
        <action
            android:id="@+id/action_fragmentCbtActivities_to_BBViewFragment"
            app:destination="@id/BBViewFragment" />
        <action
            android:id="@+id/action_fragmentCbtActivities_to_IBViewFragment"
            app:destination="@id/IBViewFragment" />
        <action
            android:id="@+id/action_fragmentCbtActivities_to_ABCViewFragment"
            app:destination="@id/ABCViewFragment" />
        <action
            android:id="@+id/action_fragmentCbtActivities_to_settingsFragment"
            app:destination="@id/settingsFragment" />
        <action
            android:id="@+id/action_fragmentCbtActivities_to_insightsFragment"
            app:destination="@id/insightsFragment" />
        <action
            android:id="@+id/action_fragmentCbtActivities_to_learnMoreFragment"
            app:destination="@id/learnMoreFragment" />
        <action
            android:id="@+id/action_fragmentCbtActivities_to_aboutFragment"
            app:destination="@id/aboutFragment" />
    </fragment>
    <fragment
        android:id="@+id/TJViewFragment"
        android:name="com.loopbreakr.brainstruct.TJViewFragment"
        android:label="fragment_thought_journal"
        tools:layout="@layout/fragment_thought_journal" />
    <fragment
        android:id="@+id/PCViewFragment"
        android:name="com.loopbreakr.brainstruct.PCViewFragnment"
        android:label="fragment_pros_and_cons"
        tools:layout="@layout/fragment_pros_and_cons" />
    <fragment
        android:id="@+id/HIGHViewFragment"
        android:name="com.loopbreakr.brainstruct.HIGHViewFragment"
        android:label="fragment_high_view"
        tools:layout="@layout/fragment_high_view" />
    <fragment
        android:id="@+id/BBViewFragment"
        android:name="com.loopbreakr.brainstruct.BBViewFragment"
        android:label="fragment_b_b_view"
        tools:layout="@layout/fragment_b_b_view" />
    <fragment
        android:id="@+id/IBViewFragment"
        android:name="com.loopbreakr.brainstruct.IBViewFragment"
        android:label="fragment_i_b_view"
        tools:layout="@layout/fragment_i_b_view" />
    <fragment
        android:id="@+id/ABCViewFragment"
        android:name="com.loopbreakr.brainstruct.ABCViewFragment"
        android:label="fragment_a_b_c_view"
        tools:layout="@layout/fragment_a_b_c_view" />
    <fragment
        android:id="@+id/settingsFragment"
        android:name="com.loopbreakr.brainstruct.SettingsFragment"
        android:label="fragment_settings"
        tools:layout="@layout/fragment_settings" />
    <fragment
        android:id="@+id/insightsFragment"
        android:name="com.loopbreakr.brainstruct.InsightsFragment"
        android:label="fragment_insights"
        tools:layout="@layout/fragment_insights" />
    <fragment
        android:id="@+id/learnMoreFragment"
        android:name="com.loopbreakr.brainstruct.LearnMoreFragment"
        android:label="fragment_learn_more"
        tools:layout="@layout/fragment_learn_more" />
    <fragment
        android:id="@+id/aboutFragment"
        android:name="com.loopbreakr.brainstruct.AboutFragment"
        android:label="fragment_about"
        tools:layout="@layout/fragment_about" />
</navigation>